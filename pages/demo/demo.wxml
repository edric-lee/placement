<!--index.wxml-->
<view class='nav' style='height:{{navH}}px'>
  <view class='nav-title'>
    <text>三年级英语测试成绩</text>
  </view>
</view>

<view class="canvas" style='margin-top:{{navH}}px'>
  <ec-canvas id="mychart-dom-bar" canvas-id="mychart-bar" ec="{{ ec }}"></ec-canvas>
</view>
<view class="box">
  <view class="canvasBox" style='height:{{winH*0.5}}px'>

  </view>
  <view class="reportBox" style='height:{{winH*0.4}}px'>
    <view class="padding flex flex-direction">
      <view class="course" >推荐课程</view>
    </view>
    <view class="divider">
      <text space="nbsp">- - - - - - - - - - - - - - -   or   - - - - - - - - - - - - - - -</text>
    </view>
    <view class="padding flex flex-direction">
      <button class='cu-btn bg-red margin-tb-sm lg shadow-blur'>查看报告</button>
    </view>
  </view>

</view>